URxvt*scrollBar: false

!-- Xft settings -- !
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight

URxvt.font: xft:Iosevka:pixelsize=14:hinting=slight
URxvt.boldfont: xft:Iosevka:pixelsize=14:hinting=slight

*.foreground:   #ebf8f9
*.background:   #000000
*.cursorColor:  #ebf8f9

! black
*.color0:       #030809
*.color8:       #021d1d

! red
*.color1:       #fc6e95
*.color9:       #f94059

! green
*.color2:       #61cab3
*.color10:      #61cab3

! yellow
*.color3:       #d9d9f3
*.color11:      #d9d9f3

! blue
*.color4:       #8dc5e6
*.color12:      #8dc5e6

! magenta
*.color5:       #b9a1c1
*.color13:      #b9a1c1

! cyan
*.color6:       #a8ddbc
*.color14:      #a1bcab

! white
*.color7:       #bfd9d7
*.color15:      #dcede5
